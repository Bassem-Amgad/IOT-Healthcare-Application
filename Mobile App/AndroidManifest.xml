<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.youssef.project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PatientActivity"
            android:label="Patient" />
        <activity
            android:name=".DoctorActivity"
            android:label="@string/category_doctors" />
        <activity
            android:name=".Doctor2Activity"
            android:label="Doctors"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Patient2Activity"
            android:label="Patient"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RegistrationActivity"
            android:label="Registration"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>